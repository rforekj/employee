<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="1.0" xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
 
    <named-query name="Department.findAllDepartment">
        <query>SELECT d FROM Department d</query>
    </named-query>
 
    <named-query name="Department.findAllDepartmentOrderByName">
        <query>SELECT d FROM Department d ORDER BY d.departmentName</query>
    </named-query>
</entity-mappings>